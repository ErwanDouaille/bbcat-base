prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@INSTALL_BIN_DIR@
libdir=@INSTALL_LIB_DIR@
includedir=@INSTALL_INCLUDE_DIR@
toolsdir=${exec_prefix}/bin

Name: @PROJECT_NAME_AND_VERSION@
Description: @PROJECT_DESCRIPTION@
Version: @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_RELEASE@.@PROJECT_VERSION_BUILD@
Libs: -L${libdir} -l@PROJECT_NAME_AND_VERSION@ @GLOBAL_LIB_DIRS_SPACES@ @GLOBAL_EXTRA_LIBS_SPACES@
Cflags: -I${includedir} @GLOBAL_FLAGS_SPACES@
